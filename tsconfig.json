{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@home/*": [
        "src/@home/*"
      ],
      "@common/*": [
        "src/@common/*"
      ],
      "@hooks/*": [
        "src/@hooks/*"
      ],
      "@utils/*": [
        "src/@utils/*"
      ],
      "@components/*": [
        "src/@components/*"
      ],
      "@app/*": [
        "src/modules/@app/*"
      ],
      "@academic/*": [
        "src/modules/@academic/*"
      ],
      "@datavalue/*": [
        "src/modules/@datavalue/*"
      ],
      "@student/*": [
        "src/modules/@student/*"
      ],
      "@repo/*": [
        "src/modules/@repo/*"
      ],
      "@exam/*": [
        "src/modules/@exam/*"
      ],
      "@vehicle/*": [
        "src/modules/@vehicle/*"
      ],
      "@employee/*": [
        "src/modules/@employee/*"
      ],
      "@dashboard/*": [
        "src/modules/@dashboard/*"
      ],
      "@library/*": [
        "src/modules/@library/*"
      ],
      "@inventory/*": [
        "src/modules/@inventory/*"
      ],
      "@notice/*": [
        "src/modules/@notice/*"
      ],
      "@billing/*": [
        "src/modules/@billing/*"
      ],
      "@account/*": [
        "src/modules/@account/*"
      ],
      "@auth/*": [
        "src/modules/@auth/*"
      ],
      "@error/*": [
        "src/modules/@error/*"
      ]
    },
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}